secrets.yaml
*.key
*.pem
ip_bans.yaml

# Database files
*.db
*.db-shm
*.db-wal
*.sqlite

# Log files
*.log
*.log.*

# Backup files
*.backup
backups/

# System/Cache files
.DS_Store
Thumbs.db
.uuid
.HA_VERSION
.storage/
.cloud/

# Dependencies & compiled files
deps/
tts/
__pycache__/
*.pyc

# Zigbee database (contains network key!)
zigbee.db

# Temporary files
*.tmp
*.temp
OZW_Log.txt

